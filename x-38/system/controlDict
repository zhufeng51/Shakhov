FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     cdugksFoam;

startFrom       latestTime;

startTime       0;//0.00990509115//0

stopAt          endTime;

endTime         2.0e2;

deltaT          4.00e-4;//7.43694832801e-09

writeControl    timeStep;

writeInterval   100;

purgeWrite      2;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   9;

runTimeModifiable true;

adjustTimeStep  yes;//no

maxCo           0.8;//0.8  0.4

maxDeltaT       1;
