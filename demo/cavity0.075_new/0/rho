FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.14423514927e-06;

boundaryField
{
    fixedWalls
    {
        type            calculatedMaxwell;
        value           uniform 8.5817636195e-07;
    }
    movingWall
    {
        type            calculatedMaxwell;
        value           uniform 8.5817636195e-07;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


