FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceTensorField;
    location    "3.67516512e-08";
    object      stressSurf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0 0 0 0);

boundaryField
{
    wall
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0 0 0 0);
    }
    inlet
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0 0 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0 0 0 0);
    }
    frontAndBackPlanes
    {
        type            empty;
        value           nonuniform 0();
    }
}


