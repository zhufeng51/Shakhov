FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      DVMProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fvDVMparas
{
    xiMax       xiMax [0 1 -1 0 0 0 0]    10;
    xiMin       xiMin [0 1 -1 0 0 0 0]   -10;
    nDV               201;       // Number of discrete velocity, shoud be 4*Z + 1 if using compound N-C quardrature
}

gasProperties
{
    R            R [0 2 -2 -1 0 0 0] 0.5; // Specific gas constant
    omega        0.5;              // VHS viscosity ~ Temperature index
    Tref         Tref [0 0 0 1 0 0 0] 2; // Reference temperature
    muRef        muRef [1 -1 -1 0 0 0 0] 0.1;
    Pr           0.666666666666667; // Prantl number
    KInner	 2;
}
